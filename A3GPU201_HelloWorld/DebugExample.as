package {	import flash.display.*;	import flash.text.*;	public class DebugExample extends MovieClip {		public function DebugExample() {			for(var i:int=0;i<10;i++) {				showNumber(i);			}		}				public function showNumber(whichNum:int) {			var myText:TextField = new TextField();			myText.text = String(whichNum);			myText.y = whichNum*20;			addChild(myText);		}	}}